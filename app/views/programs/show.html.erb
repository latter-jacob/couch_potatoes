<div class="flex-video widescreen">
  <iframe width="854" height="480" src="//www.youtube.com/embed/<%= @program.url %>" frameborder="0"></iframe>
</div>

<div class="medium-9 columns">

  <h1 class="show-title"><%=@program.title%></h1>

  Rating: <%= rating_for @program, "rating", disable_after_rate: false %>

  <a href="#" class="button" id="review-button">Add a review</a>

  <div class="hide" id="add-review">
    <%= render "programs/add_review" %>
  </div>

    <%= render "programs/reviews" %>
    <%= paginate @reviews %>
</div>

<div class="medium-3 columns">
    <%= render "layouts/search_bar" %>
  <h3>Watch Next</h3>
  <img src="http://img.youtube.com/vi/ftrwe16sBiA/mqdefault.jpg" /> </br ></br >
  <img src="http://img.youtube.com/vi/ftrwe16sBiA/mqdefault.jpg" /><br /></br >
  <img src="http://img.youtube.com/vi/ftrwe16sBiA/mqdefault.jpg" />
</div>
